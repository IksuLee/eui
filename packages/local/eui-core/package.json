{
    "name": "eui-core",
    "namespace": "Eui.core",
    "type": "code",
    "toolkit": "classic",
    "framework": "ext",
    "creator": "anonymous",
    "summary": "Short summary",
    "detailedDescription": "Long description of package",
    "version": "1.0.1",
    "compatVersion": "1.0.1",
    "format": "1",
    "slicer": {
        "js": [
            {
                "path": "${package.dir}/sass/example/custom.js",
                "isWidgetManifest": true
            }
        ]
    },
    "output": "${package.dir}/build",
    "local": true,
    "sass" : {
        "namespace": "Eui.core",
        "etc": "${package.dir}/sass/etc/all.scss,${package.dir}/${toolkit.name}/sass/etc/all.scss",
        "var": "${package.dir}/sass/var,${package.dir}/${toolkit.name}/sass/var",
        "src": "${package.dir}/sass/src,${package.dir}/${toolkit.name}/sass/src"
    },
    "classpath": "${package.dir}/src,${package.dir}/${toolkit.name}/src",
    "overrides": "${package.dir}/overrides,${package.dir}/${toolkit.name}/overrides",
    "requires": [],
    "architect":{
        "compatFrameworks": [
            "ext60"
        ],
        "classes": [
            {
                "definition": "button/Button.js",
                "className": "eui.button.Button",
                "js": [
                    "../build/eui-core-debug.js"
                ],
                "css": [
                ]
            },
            {
                "definition": "form/field/Text.js",
                "className": "eui.form.field.Text",
                "js": [
                    "../build/eui-core-debug.js"
                ],
                "css": [
                ]
            },
            {
                "definition": "form/field/Date.js",
                "className": "eui.form.field.Date",
                "js": [
                    "../build/eui-core-debug.js"
                ],
                "css": [
                ]
            },
            {
                "definition": "form/Panel.js",
                "className": "eui.form.Panel",
                "js": [
                    "../build/eui-core-debug.js"
                ],
                "css": [
                    "eui-core-all.css"
                ]
            }
        ]
    }
}
